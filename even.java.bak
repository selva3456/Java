import java.util.*;
class even

{
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) 
	{
		System.out.print("\t\t\tEnter number:");
		int number=sc.nextInt(),sum=0;
		while(number>0){
			int last=number%10;
			if(last%2==0)
				sum=sum+last;
			number/=10;
		}
		System.out.println("Total Even Digit"+sum);
	}
}
		