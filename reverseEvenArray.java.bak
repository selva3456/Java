import java.util.*;
class reverseEvenArray 
{
	public static void main(String[] args) 
	{
		int a[]={26,32,17,64,84,77};
		int []rev=new int[a.length];
		for (int i=0;i<a.length;i++)
		{
			if (a[i]%2==0){
			int temp=a[i];
			int reverse=0;
			
			 while(a[i]>0){
				reverse=(reverse*10)+(a[i]%10);
			    a[i]/=10;
			 }
			rev[i]=reverse;
			}else{
				rev[i]=a[i];
			}
		}
		System.out.println(Arrays.toString(rev));
	}
}
