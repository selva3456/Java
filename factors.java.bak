import java.util.*;
class factors 
{
	static Scanner sc=new Scanner(System.in);
	public static void factors(int n)
	{
		for (int i=1;i<=n/2;i++)
	   {
		   if(n%i==0)
			   System.out.println("Factor of number is:"+i);
	   }
	   return 2;
}
          public static void main(String[]args)
	{
		System.out.print("Enter number:");
		int number=sc.nextInt();
		factors(number);
		
	}
}
