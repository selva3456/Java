class kadane 
{
	public static void main(String[] args) 
	{
		int[]a={2,-3,7,-4,6};
		System.out.println(kadane(a));
		
	}
	public static int kadane(int[]a)
	{
		int max_sum=Integer.MIN_VALUE;
		int cur_sum=0;
		for (int i=0;i<a.length ;i++ )
		{
			cur_sum+=a[i];
			max_sum=Math.max(cur_sum,max_sum);
			if (cur_sum<0)
			  cur_sum=0;
		}
		return max_sum;
	}
}
