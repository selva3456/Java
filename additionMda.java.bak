class additionMda
{
	public static void main(String[] args) 
	{
		int[][]a={
			{4,1,2},
			{7,5,3},
			{1,4,8},
		};
	    int[][]b={
			{6,7,3},
			{2,6,1},
			{9,2,7},
		};
		int[][]res=new int[3][3];
		for (int i=0;i<a.length ;i++ )
		{
			for (intt j=0;j<a[i].length ;j++ )
			{
				res[i][j]=a[i][j]+b[i][j];
				System.out.print(res[i][j]+" ");
			}
				System.out.println();
		}
	}
}
