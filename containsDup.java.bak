import java.util.*;
class containsDup
{
	public static void main(String[] args) 
	{
		int[]a={1,8,7,2,3,8,9};
		System.out.println(Arrays.toString(a));
		System.out.println("count:"+containsDup(a));
	}
	public static int containsDup(int[]a)
	{
		int count=0;
		Arrays.sort(a);
		for (int i=0;i<a.length-1;i++ ) 
		{
			if(a[i]==a[i+1])
				count++;
		}
		return count;
	}
}
