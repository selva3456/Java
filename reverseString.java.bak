class reverseString 
{
	public static void main(String[] args) 
	{
		String s="SpiDeR";
		System.out.println(s);
		System.out.println(solve(s));
	}
	public static String solve(String s)
	{
		char[]a=s.toCharArray();
		for (int i=0;i<a.length ;i++ )
		{
			if(a[i]>='A' && a[i]<='Z')
				a[i]+=32;
			else if(a[i]>='a' && a[i]<='z')
				a[i]-=32;
		}
		return new String(a);
	}
	
}
